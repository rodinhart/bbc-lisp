;; 24 (25) bit floats?

oswrch = &FFEE
osasci = &FFE3
osnewl = &FFE7

ret = &70
exp = &72
env = &74
tmp = &76

INCLUDE "macros.6502"

ORG &2000

.start
    JSR freeInit
    LDX #0 ; init stack
    JSR freeReport

    ADDR exp, source
    JSR read

    PUSH ret
    MOVE exp, ret
    JSR print
    JSR osnewl

    PULL exp
    ADDR env, core
    JSR eval
    
    MOVE exp, ret
    JSR print
    JSR osnewl

    RTS

.source
    EQUB "((fn (x y) (+ y y y)) 3 4)", 0

INCLUDE "core.6502"
INCLUDE "eval.6502"
INCLUDE "gc.6502"
INCLUDE "print.6502"
INCLUDE "read.6502"
INCLUDE "types.6502"
INCLUDE "util.6502"

ALIGN &100
.stack_low
    SKIP 256
ALIGN &100
.stack_high
    SKIP 256
.end

SAVE "Lisp", start, end
